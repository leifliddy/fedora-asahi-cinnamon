alias gospec='cd ~/rpmbuild/SPECS'
alias gosource='cd ~/rpmbuild/SOURCES'
alias gobuild='cd ~/rpmbuild/BUILD'
alias gobuildroot='cd ~/rpmbuild/BUILDROOT'
alias gorpm='cd ~/rpmbuild/RPMS'
alias gosrpm='cd ~/rpmbuild/SRPMS'

dep() {
   dnf builddep $1
}

bp() {
  rpmbuild -bp $1
}

bb() {
  rpmbuild -bb $1
}

ba() {
  rpmbuild -ba $1
}
