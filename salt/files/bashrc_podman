
alias dps='podman ps -a'
alias drm='podman rm'
alias drmi='podman rmi'

# the docker.io registry has a more up-to-date centos7 image then the centos registry
alias startcentos7='podman run --rm -ti -h centos7 docker.io/library/centos:centos7 /bin/bash'
alias startrocky8='podman run --rm -ti -h rockylinux8 docker.io/library/rockylinux:8 /bin/bash'
alias startrocky='podman run --rm -ti -h rockylinux9 docker.io/library/rockylinux:9 /bin/bash'
alias startfedora36='podman run --rm -ti -h fedora36 registry.fedoraproject.org/fedora:36 /bin/bash'
alias startfedora37='podman run --rm -ti -h fedora37 registry.fedoraproject.org/fedora:37 /bin/bash'
alias startfedora38='podman run --rm -ti -h fedora38 registry.fedoraproject.org/fedora:38 /bin/bash'
alias startfedora='podman run --rm -ti -h fedora39 registry.fedoraproject.org/fedora:39 /bin/bash'
